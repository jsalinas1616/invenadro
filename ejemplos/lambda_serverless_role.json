{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup"
      ],
      "Resource": [
        "arn:aws:logs:us-east-1:573066395874:log-group:/aws/lambda/nadro-useast1-prod-account-statement-function",
        "arn:aws:logs:us-east-1:573066395874:log-group:/aws/lambda/nadro-useast1-prod-credit-limit-function",
        "arn:aws:logs:us-east-1:573066395874:log-group:/aws/lambda/nadro-useast1-prod-next-delivery-function"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": [
        "arn:aws:logs:us-east-1:573066395874:log-group:/aws/lambda/nadro-useast1-prod-account-statement-function:*",
        "arn:aws:logs:us-east-1:573066395874:log-group:/aws/lambda/nadro-useast1-prod-credit-limit-function:*",
        "arn:aws:logs:us-east-1:573066395874:log-group:/aws/lambda/nadro-useast1-prod-next-delivery-function:*"
      ]
